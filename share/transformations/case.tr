# Upper/Lower-case rules
#
# See basic.tr for formatting options

ascii_lowercase:
U+0041 - U+005A + 20

ascii_uppercase:
U+0061 - U+007A - 20

latinbasic_lowercase:
U+00C0 - U+00D6 + 20 # Lowercase diacritical latin basic characters
U+00D8 - U+00DE + 20 # Lowercase diacritical latin basic characters

latinbasic_uppercase:
U+00E0 - U+00F6 - 20 # Lowercase diacritical latin basic characters
U+00F8 - U+00FE - 20 # Lowercase diacritical latin basic characters

