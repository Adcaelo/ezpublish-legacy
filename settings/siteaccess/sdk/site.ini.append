#?ini charset="iso-8859-1"?
# eZ publish configuration file.

[DesignSettings]
SiteDesign=standard

[SiteSettings]
DefaultPage=/sdk
ErrorHandler=defaultpage
IndexPage=/sdk

[SiteAccessSettings]
RequireUserLogin=false

[SiteAccessRules]
Access=disable
ModuleAll=true
Access=enable
Module=sdk

[SitePrecheckRules]
Access=disable
PreCheck=user
