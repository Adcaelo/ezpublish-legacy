{default related_view="line" container="div"}
<{$container} class='related_object'>
{section show=and(is_array($attribute.content),count($attribute.content.id_list))}
  {section name=Relation loop=$attribute.content.id_list show=ne($attribute.content.id_list[0],0)}
    {let related=fetch(content,object,hash(object_id,$:item))}
      {node_view_gui view=$related_view content_node=$:related.main_node}
    {/let}
  {/section}
{/section}
</{$container}>
{/default}
